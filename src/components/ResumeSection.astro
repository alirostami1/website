---
// title prop

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<h2>{title}</h2>
<hr />
<style>
    h2 {
        margin-top: 2rem;
        margin-bottom: 1rem;
        font-size: 2rem;
        font-weight: 600;
    }
    hr {
        margin-top: 0.5rem;
        margin-bottom: 1rem;
        border-top: 1.2px solid #787878;
    }
</style>
